    "import math \n",
    "Seqnum = int(input('Enter the desired sequence number of Fibonacci number '))\n",
    "Golrat = 1.61803398875\n",
    "root5 = math.sqrt(5)\n",
    "Fibnum = (((Golrat)**Seqnum)-((Golrat - 1)**Seqnum))/root5\n",
    "print(Fibnum)"
